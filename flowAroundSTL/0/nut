FoamFile
{
	version     2.0;
	format      ascii;
	class       volScalarField;
	location    "0";
	object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

nut_0 SED_NUT_0;

dimensions [0 2 -1 0 0 0 0];

internalField uniform $nut_0;

boundaryField
{
	walls
	{
		type  nutkWallFunction;
		value uniform $nut_0;
	}

	inlet
	{
		type  nutkWallFunction;
		value uniform $nut_0;
	}

	outlet
	{
		type  nutkWallFunction;
		value $internalField;
	}

	SED_MODEL_NAME
	{
		type  nutkWallFunction;
		value uniform $nut_0;
	}
  
	frontAndBack
	{
		type empty;
	}
}


