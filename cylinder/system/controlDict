FoamFile
{
	version     2.0;
	format      ascii;
	class       dictionary;
	location    "system";
	object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../constant/parameters"

application     simpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         $end_time;

deltaT          $delta_t;

//maxCo           0.5; // Or other Courant number you wish

adjustTimeStep  no; // yes or no 

//writeControl    adjustableRunTime;
writeControl    runTime;
//writeControl    timeStep;

writeInterval   0.25;
//timeInterval    1;
//writeFrequency  1;

purgeWrite      0;

writeFormat     binary; //or ascii

writePrecision  8;

timeFormat      general;

timePrecision   10;

runTimeModifiable true;

writeCompression  off;

